{
  "name": "riko",
  "version": "1.0.0",
  "description": "Webpack Build System for React JS Projects",
  "main": "riko.js",
  "scripts": {
    "start": "export NODE_ENV=production && webpack",
    "dev": "export NODE_ENV=development && babel-node server.js",
    "prod": "export NODE_ENV=production && webpack",
    "test": "export NODE_ENV=development && karma start --single-run --no-auto-watch && jest",
    "test-watch": "node_modules/.bin/karma start --auto-watch --no-single-run",
    "test-jest": "jest",
    "server": "node server/server.js",
    "prepare": "sudo npm install -g webpack"
  },
  "author": "Don Mclean",
  "license": "MIT",
  "jest": {
    "automock": false,
    "collectCoverage": true,
    "rootDir": "./",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "devDependencies": {
