{
  "name": "riko",
  "version": "1.0.0",
  "description": "Webpack Build System for React JS Projects",
  "main": "riko.js",
  "scripts": {
    "start": "export NODE_ENV=production && webpack",
    "dev": "export NODE_ENV=development && webpack -w --display-error-details",
    "prod": "export NODE_ENV=production && webpack",
    "test": "node_modules/.bin/karma start --single-run --no-auto-watch",
    "test-watch": "node_modules/.bin/karma start --auto-watch --no-single-run",
    "server": "node server/server.js",
    "prepare": "sudo npm install -g webpack",
    "new":"export NODE_ENV=development && node app"
  },
  "author": "Don Mclean",
  "license": "MIT",
