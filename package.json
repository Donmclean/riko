{
  "name": "riko",
  "version": "2.0.0",
  "description": "Webpack Build System for React JS Projects (web & electron)",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run prod && npm run prod-server",
    "dev": "cross-env NODE_ENV=development babel-node server.js",
    "electron": "cross-env NODE_ENV=development ELECTRON=true babel-node server.js",
    "electron-dev": "cross-env NODE_ENV=development ELECTRON=true electron -r babel-register ./src/electron.js ",
    "electron-prod": "cross-env NODE_ENV=production ELECTRON=true babel-node server.js",
    "prod": "cross-env NODE_ENV=production webpack",
    "test": "npm run test-build && gulp lint-src && NODE_ENV=test jest",
    "test-jest": "jest",
    "test-build": "gulp lint-build && NODE_ENV=production mocha test-riko/riko.spec.js",
    "test-complete": "sh test-complete.sh",
    "server": "NODE_ENV=production babel-node server.js",
    "prod-server": "NODE_ENV=production LIVE=true babel-node server.js",
    "setup": "gulp setup --src",
    "setup-demo": "gulp setup --demo",
    "setup-electron": "gulp setup --electron",
    "vorlon": "PORT=1337 vorlon",
    "test-coverage": "NODE_ENV=production istanbul cover ./node_modules/mocha/bin/_mocha test-riko/riko.spec.js --report lcovonly -- -R spec && codeclimate-test-reporter < ./coverage/lcov.info && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Donmclean/riko.git"
  },
  "homepage": "https://github.com/Donmclean/riko",
  "keywords": [
    "javascript",
    "react",
    "webpack",
    "electron",
    "test",
    "coverage"
  ],
  "author": "Don Mclean",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "automock": false,
    "collectCoverage": true,
    "coverageDirectory": "test-coverage",
    "rootDir": "./",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(src/__tests__/.*|\\.(test|spec))\\.(es6|js|jsx)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|sass|scss)$": "<rootDir>/src/__tests__utils/styleMock.js",
      "^.+\\.(jpe?g|png|gif|tif|svg|bmp|woff|woff2|eot|ttf|mp4|doc|docx|pdf|xls|xlsx|csv|txt)$": "<rootDir>/src/__tests__utils/fileMock.js"
    },
    "testPathDirs": [
      "<rootDir>/src/__tests__"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__utils/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__utils/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "browser-sync": "^2.14.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.10",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "electron": "^1.4.5",
    "electron-packager": "^8.2.0",
    "eslint": "^3.3.0",
    "eslint-loader": "1.5.0",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-debug": "^3.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.22.0",
    "imagemin-webpack-plugin": "^1.1.0",
    "ip": "^1.1.3",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.4.1",
    "jest": "^17.0.0",
    "jest-cli": "^17.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.15.0",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^1.0.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "pug": "^2.0.0-beta5",
    "pug-loader": "^2.2.1",
    "q": "^1.4.1",
    "q-io": "^1.13.2",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.3.1",
    "react-tools": "^0.13.3",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.3.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "stylelint-config-standard": "^14.0.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "vorlon": "^0.4.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-notifier": "^1.4.1",
    "webpack-shell-plugin": "^0.4.3",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0"
  }
}
