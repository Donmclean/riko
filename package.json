{
  "name": "riko",
  "version": "1.0.0",
  "description": "Webpack Build System for React JS Projects",
  "main": "riko.js",
  "scripts": {
    "start": "export NODE_ENV=production && webpack",
    "dev": "export NODE_ENV=development && babel-node server.js",
    "prod": "gulp clean && export NODE_ENV=production && webpack",
    "test": "export NODE_ENV=development && karma start --single-run --no-auto-watch && jest && gulp run-selenium-tests",
    "test-karma": "export NODE_ENV=development && karma start --single-run --no-auto-watch",
    "test-watch": "karma start --auto-watch --no-single-run",
    "test-jest": "jest",
    "test-selenium": "gulp run-selenium-tests",
    "clean": "gulp clean",
    "server": "export NODE_ENV=production && babel-node server.js"
  },
  "author": "Don Mclean",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "automock": false,
    "collectCoverage": true,
    "coverageDirectory": "test-coverage",
    "rootDir": "./",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|sass|scss)$": "<rootDir>/src/__tests__utils/styleMock.js",
      "^.+\\.(jpe?g|png|gif|tif|svg|bmp|woff|woff2|eot|ttf|mp4|doc|docx|pdf|xls|xlsx|csv|txt)$": "<rootDir>/src/__tests__utils/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__utils/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "browser-sync": "^2.14.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.2",
    "eslint": "^3.3.0",
    "eslint-loader": "^1.5.0",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-debug": "^2.1.2",
    "gulp-load-plugins": "^1.2.4",
    "gulp-nightwatch": "^0.3.1",
    "gulp-plumber": "^1.1.0",
    "html-webpack-plugin": "^2.22.0",
    "imagemin-webpack-plugin": "^1.1.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jest": "^14.1.0",
    "jest-cli": "^14.1.0",
    "karma": "^1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.15.0",
    "mocha": "^3.0.2",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0",
