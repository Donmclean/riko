{
  "verbose": true,
  "automock": false,
  "collectCoverage": true,
  "collectCoverageFrom": ["src/js/**/*.js", "src/js/**/*.jsx", "!**/vendor/**"],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "coverageDirectory": "test-coverage",
  "rootDir": "./",
  "transform": {
    ".*": "<rootDir>/node_modules/babel-jest"
  },
  "testRegex": "(src/*|\\.(test|spec))\\.(es6|js|jsx)$",
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json"
  ],
  "moduleNameMapper": {
    "^.+\\.(css|less|sass|scss)$": "<rootDir>/src/__tests__utils/styleMock.js",
    "^.+\\.(jpe?g|png|gif|tif|svg|bmp|woff|woff2|eot|ttf|mp4|doc|docx|pdf|xls|xlsx|csv|txt)$": "<rootDir>/src/__tests__utils/fileMock.js"
  },
  "testPathDirs": [
    "<rootDir>/src/"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/__tests__utils/"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/__tests__utils/"
  ]
}