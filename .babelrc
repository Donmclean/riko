{
  "presets": [
    "es2015",
    ["env", {
      "targets": {
        "node": true
      }
    }]
  ],
  "plugins": [
    "dynamic-import-node",
    "syntax-dynamic-import",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}